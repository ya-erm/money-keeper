{
  "name": "client",
  "version": "2.8.6",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "start": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:codegen": "npx playwright codegen http://localhost:5173",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "tsc --noemit && prettier --check . && eslint .",
    "format": "prettier --write .",
    "generate:icons": "node ./pack-icons.js && prettier --write ./src/lib/icons.ts",
    "generate:prisma": "npx prisma generate",
    "generate": "npm run generate:icons && npm run generate:prisma"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.205",
    "@iconify/svelte": "^3.1.6",
    "@iconify/utils": "^2.1.23",
    "@playwright/test": "^1.43.1",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/adapter-node": "^4.0.1",
    "@sveltejs/kit": "^2.5.7",
    "@types/bcrypt": "^5.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vite-pwa/sveltekit": "^0.5.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.38.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "prisma": "^5.13.0",
    "svelte": "^4.2.19",
    "svelte-check": "^3.7.0",
    "svelte-i18n": "^4.0.0",
    "svelte-preprocess": "^5.1.4",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10"
  },
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "@vercel/analytics": "^1.2.2",
    "bcrypt": "^5.1.1",
    "dayjs": "^1.11.11",
    "idb": "^8.0.0",
    "svelte-dnd-action": "^0.9.43",
    "timezones-list": "^3.0.3",
    "uuid": "^9.0.1"
  },
  "type": "module"
}
